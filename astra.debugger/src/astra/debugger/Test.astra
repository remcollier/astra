package astra.debugger;

import astra.gui.GuiModule;

agent Test {
	types test {
		formula light(string);
	}
	
	initial light("off");

	initial !turn("on");
		
	rule +!turn("on") : light("off") {
		-light("off");
		+light("on");
		!!turn("off");
	}
	
	rule +!turn("off") : light("on") {
		-light("on");
		+light("off");
		!!turn("on");
	}
}
